
<body>

    <div class="container">
        <div class="row">
            <div class="col-md-6 mx-auto">

                

                <form action="/elearning/api/v1/admin/register" method="post">
                    <h1 class="d-flex justify-content-center">REGISTRATION</h1>
                    <div class="row my-4">
                        <div class="col-md-12">
                            <label for="name">Name</label>
                            <input class="form-control" type="text" name="name" placeholder="Name">
                        </div>
                    </div>


                    <div class="row my-4">
                        <div class="col-md-12">
                            <label for="Email">Email ID</label>
                            <input class="form-control" type="text" name="email_id" placeholder="Email">
                        </div>
                    </div>

    
                    <div class="row my-4">
                        <label for="Username">Username</label>
                        <div class="col-md-12"><input class="form-control" type="text" name="user_name"
                                placeholder="Username"></div>
                    </div>

                    <div class="row my-4">
                        <label for="Password">Password</label>
                        <div class="col-md-12"><input class="form-control" type="password" name="password"
                                placeholder="Password"></div>
                    </div>

                    <div class="row my-4">
                        <label for="Confirm Password">Confirm Password</label>
                        <div class="col-md-12"><input class="form-control" type="password" name="confirm_password"
                            placeholder="Confirm Password">
                        </div>
                        
                    </div>

                    <div class="row my-4">
                        <div class="col-md-12">
                            <button type="submit" class="bg-dark btn btn-primary w-100">Register</button>
                        </div>
                    </div>

                </form>
            </div>
        </div>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
        crossorigin="anonymous"></script>

</body>

</html>
